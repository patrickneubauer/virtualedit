<?xml version="1.0" encoding="ASCII"?>
<router:Container xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:router="http://router/1.0">
  <agents xsi:type="router:Provider" name="Blas1" speed="3"/>
  <agents xsi:type="router:Provider" name="Blas2" speed="3"/>
  <agents xsi:type="router:Consumer" name="Blapc1" speed="2" rightcomp="//@devices.4"/>
  <agents xsi:type="router:Consumer" name="Blapc3" speed="2" rightcomp="//@devices.5"/>
  <devices xsi:type="router:Server" name="Blas1">
    <outgoing xsi:type="router:GlassFiberCable" speed="4" target="//@devices.2"/>
  </devices>
  <devices xsi:type="router:Server" name="Blas2">
    <outgoing xsi:type="router:GlassFiberCable" speed="4" target="//@devices.3"/>
  </devices>
  <devices xsi:type="router:Router" incoming="//@devices.0/@outgoing.0" name="Blar1" maxSpeed="4">
    <outgoing xsi:type="router:CopperCable" speed="2" target="//@devices.4"/>
  </devices>
  <devices xsi:type="router:Router" incoming="//@devices.1/@outgoing.0" name="Blar2" maxSpeed="4">
    <outgoing xsi:type="router:GlassFiberCable" speed="2" target="//@devices.5"/>
  </devices>
  <devices xsi:type="router:Computer" incoming="//@devices.2/@outgoing.0" name="Blapc1" consumer="//@agents.2"/>
  <devices xsi:type="router:Computer" incoming="//@devices.3/@outgoing.0" name="Blapc3" consumer="//@agents.3"/>
</router:Container>
